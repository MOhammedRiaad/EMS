{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": "test/.*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^uuid$": "uuid"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!uuid)"
  ],
  "testTimeout": 60000
}